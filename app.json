  "repository": "https://github.com/BandwidthExamples/node-typescript-webrtc.git",
  "logo": "https://avatars2.githubusercontent.com/u/12057884?v=3&s=200",
  "keywords": ["js", "node", "bandwidth", "catapult", "typescript", "nodejs", "webrtc"],
  "addons": ["mongolab:sandbox"],
  "env": {
    "CATAPULT_USER_ID": {
      "description": "Your Catapult User Id"
